@model ProektTRPO.ViewModels.KeysModel
<form asp-action="Create" asp-controller="Account">
    <div class="form-group">
        <label asp-for="Keys" class="control-label">Ключ</label>
        <input type="text" asp-for="Keys" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="EncryptedData" class="control-label">Зашифрованная информация</label>
        <input type="text" asp-for="EncryptedData" class="form-control" />
    </div>

    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-default" />
    </div>
</form>